{
  "name": "carsventures",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-plugin-styled-components": "^1.6.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scripts": "1.1.5",
    "styled-components": "^3.4.5"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/**",
    "lint:staged": "lint-staged",
    "start": "babel-node src/index.js",
    "prettier": "prettier --write --single-quote true --trailing-comma all --print-width 120",
    "prettier:all": "prettier --write --single-quote true --trailing-comma all --print-width 120 --write 'src/*/.js'"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2"
  }
}
